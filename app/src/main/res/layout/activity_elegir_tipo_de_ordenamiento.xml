<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.edojs.etecapp.ElegirTipoDeOrdenamientoYPeticionActivity">


    <RelativeLayout
        android:layout_width="330dp"
        android:layout_height="495dp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp">


        <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_above="@+id/button4"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:text="ELIJA EL TIPO DE ORDENAMIENTO"
            android:textAppearance="@style/TextAppearance.AppCompat.Button"
            android:visibility="visible" />

        <Button
            android:id="@+id/button4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/button6"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="26dp"
            android:layout_toLeftOf="@+id/button5"
            android:layout_toStartOf="@+id/button5"
            android:onClick="tipoDeOrdenamientoAscendente"
            android:text="ASCENDENTE"
            tools:layout_editor_absoluteX="-86dp"
            tools:layout_editor_absoluteY="181dp" />

        <Button
            android:id="@+id/button5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="tipoDeOrdenamientoDescendente"
            android:text="DESCENDENTE"
            android:layout_alignBottom="@+id/button4"
            android:layout_alignRight="@+id/textView8"
            android:layout_alignEnd="@+id/textView8" />



        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/button6"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:text="ELIJA EL ALGORITMO DE BUSQUEDA DESEADO"
            android:textAppearance="@style/TextAppearance.AppCompat.Button"
            android:visibility="visible" />

        <Button
            android:id="@+id/button2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/button9"
            android:onClick="btnBubleSort"
            android:text="BUBLESORT" />

        <Button
            android:id="@+id/button3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="QUICKSORT"
            android:layout_below="@+id/button9"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />



        <Button
            android:id="@+id/button6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Radix Sort"
            android:layout_above="@+id/button10"
            android:layout_alignLeft="@+id/button2"
            android:layout_alignStart="@+id/button2" />

        <Button
            android:id="@+id/button7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Merge Sort"
            android:layout_above="@+id/button10"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <Button
            android:id="@+id/button9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Insertion Sort"
            android:layout_below="@+id/button7"  />

        <Button
            android:id="@+id/button10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginBottom="106dp"
            android:text="Selection Sort" />

        <Button
            android:id="@+id/button17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/button9"
            android:layout_centerHorizontal="true"
            android:text="Shell Sort" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Elija el parametro de busqueda deceado"
            android:textAppearance="@style/TextAppearance.AppCompat.Button"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="73dp"
            android:layout_above="@+id/button11"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <Button
            android:onClick="parametroDeBusquedaPrecio"
            android:id="@+id/button11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/textView9"

            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="22dp"
            android:text="POR PRECIO" />

        <Button
            android:onClick="parametroDeBusquedaAlfabetico"
            android:id="@+id/button12"
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:text="POR ORDEN ALFABETICO"
            android:layout_alignTop="@+id/button11"
            android:layout_toRightOf="@+id/button9"
            android:layout_toEndOf="@+id/button9" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_above="@+id/button14"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:text="Elija el tipo de busqueda"
            android:textAppearance="@style/TextAppearance.AppCompat.Button" />

        <Button
            android:onClick="tipoDeBusquedaBinaria"
            android:id="@+id/button14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="busqueda Binaria"
            android:layout_alignBottom="@+id/button15"
            android:layout_toRightOf="@+id/button15"
            android:layout_toEndOf="@+id/button15"
            android:layout_alignTop="@+id/button15" />

        <Button
            android:onClick="tipoDeBusquedaInterpolacion"
            android:id="@+id/button15"
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:layout_above="@+id/textView3"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="15dp"
            android:text="Busqueda por interpolacion" />





    </RelativeLayout>

</android.support.constraint.ConstraintLayout>
